{
  "kind": "collectionType",
  "collectionName": "decks",
  "info": {
    "name": "Decks"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "flashcards": {
      "via": "decks",
      "collection": "flashcard",
      "dominant": true
    },
    "tags": {
      "via": "decks",
      "collection": "tags"
    },
    "users": {
      "via": "decks",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
