{
  "kind": "collectionType",
  "collectionName": "decks",
  "info": {
    "name": "Decks"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "flashcards": {
      "via": "decks",
      "collection": "flashcard",
      "dominant": true
    },
    "tags": {
      "collection": "tags",
      "via": "decks"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "publish"
      ],
      "default": "pending",
      "required": true
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "decks"
    },
    "description": {
      "type": "string"
    }
  }
}
